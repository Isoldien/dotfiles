font_size 10
# cursor #fc034e
# url_color #fc0324
url_style curly
sync_to_monitor yes
remember_window_size yes
window_padding_width 30
# active_border_color none
hide_window_decorations yes
# window_logo_path /home/habib/Pictures/pika.png
window_logo_position top-right
window_logo_scale 25
window_logo_alpha 0.05
